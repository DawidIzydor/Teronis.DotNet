<Project>

  <PropertyGroup>    
    <TeronisMSBuildPackagingPackShadowProjectPropsHasBeenImported>true</TeronisMSBuildPackagingPackShadowProjectPropsHasBeenImported>
    <ShadowProjectReference>$(ShadowProjectReference)</ShadowProjectReference>
    <CopyShadowSourceProjectFilesDependsOn>$(CopyShadowSourceProjectFilesDependsOn)</CopyShadowSourceProjectFilesDependsOn>
    <ProjectShadowGitIgnoreFile Condition="'$(ProjectShadowGitIgnoreFile)' == ''">$(MSBuildThisFileDirectory)$(MSBuildThisFileName).gitignore</ProjectShadowGitIgnoreFile>
    <DisableProjectShadowGitIgnoreCopy Condition="'$(DisableProjectShadowGitIgnore)' == ''">false</DisableProjectShadowGitIgnoreCopy>
  </PropertyGroup>
  
</Project>
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DisableGitVersionCache>true</DisableGitVersionCache>
  </PropertyGroup>

  <ItemGroup>
    <GenerateFilesystemPackageProjects Include="..\GitVersionCache\*.csproj" />
  </ItemGroup>

  <Import Project="$(TeronisMSBuildCorePackagingGenerateFilesystemPackageInProjectTargetsFile)" />

</Project>
